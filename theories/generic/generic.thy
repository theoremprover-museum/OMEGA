(in-package :omega)

(th~deftheory GENERIC
	      (help "Generic theory containing things applicable in each logic."))

(th~deftype o
	    (in generic)
	    (arguments 0)
	    (help "The type of truth values"))

(th~deftype i
	    (in generic)
	    (arguments 0)
	    (help "The type of individuals"))

(th~deftype list
	    (in generic)
	    (arguments 0)
	    (help "A simple type of lists"))

(th~defconstant =def
  (in GENERIC)
  (type (all-types AA ((O  AA ) AA )))
  (help "OMEGA constant for local definitions.")
  )

		

