(in-package :omega)


(th~defproblem taut-res-complete
	(in taut-res)
	(conclusion theo (forall (lam (klm (o (o form)))
			    (implies (in klm all-clause-sets)
				     (implies (and (unsat-cl-set klm)
						   (finite klm))
					      (derivable empty-cl klm crule-set)))))))
